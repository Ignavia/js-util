{
  "name": "@ignavia/util",
  "version": "1.4.0",
  "description": "Some utility classes.",
  "keywords": [
    "utility"
  ],
  "homepage": "http://ignavia.github.io/js-util",
  "bugs": "https://github.com/Ignavia/js-util/issues",
  "license": "MIT",
  "author": "Lars Reimann <mail@lars-reimann.eu> (http://lars-reimann.eu)",
  "files": [
    "dist/"
  ],
  "main": "dist/util.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ignavia/js-util.git"
  },
  "scripts": {
    "build": "jspm build ./src/util.js - lodash ./dist/util.js --format cjs --skip-source-maps --minify",
    "doc": "esdoc -c esdoc.json",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {
    "lodash": "^4.6.1"
  },
  "devDependencies": {
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "chai-string": "^1.1.6",
    "core-js": "^2.1.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "es2015"
        ],
        "plugins": [
          "transform-export-extensions",
          "transform-object-rest-spread"
        ]
      }
    }
  },
  "jspmPackage": true,
  "registry": "npm",
  "format": "cjs",
  "jspm": {
    "name": "@ignavia/util",
    "dependencies": {
      "lodash": "^4.6.1"
    },
    "devDependencies": {
      "babel-plugin-transform-export-extensions": "^6.5.0",
      "babel-plugin-transform-object-rest-spread": "^6.6.5",
      "babel-regenerator-runtime": "^6.5.0",
      "plugin-babel": "systemjs-plugin-babel@^0.0.8"
    },
    "peerDependencies": {
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "core-js": "^1.2.0",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha"
    }
  }
}
